{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "array",
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "id",
      "name",
      "slug",
      "city",
      "state",
      "createdAt",
      "updatedAt"
    ],
    "properties": {
      "id": {
        "$id": "#/items/properties/id",
        "type": "integer",
        "title": "The Id Schema",
        "default": 0,
        "examples": [
          1
        ]
      },
      "name": {
        "$id": "#/items/properties/name",
        "type": "string",
        "title": "The Name Schema",
        "default": "",
        "examples": [
          "string"
        ],
        "pattern": "^(.*)$"
      },
      "slug": {
        "$id": "#/items/properties/slug",
        "type": "string",
        "title": "The Slug Schema",
        "default": "",
        "examples": [
          "string"
        ],
        "pattern": "^(.*)$"
      },
      "city": {
        "$id": "#/items/properties/city",
        "type": "string",
        "title": "The City Schema",
        "default": "",
        "examples": [
          "string"
        ],
        "pattern": "^(.*)$"
      },
      "state": {
        "$id": "#/items/properties/state",
        "type": "string",
        "title": "The State Schema",
        "default": "",
        "examples": [
          "AC"
        ],
        "pattern": "^(.*)$"
      },
      "createdAt": {
        "$id": "#/items/properties/createdAt",
        "type": "string",
        "title": "The Createdat Schema",
        "default": "",
        "examples": [
          "2019-08-18T19:59:14"
        ],
        "pattern": "^(.*)$"
      },
      "updatedAt": {
        "$id": "#/items/properties/updatedAt",
        "type": "string",
        "title": "The Updatedat Schema",
        "default": "",
        "examples": [
          "2019-08-18T19:59:14"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}


