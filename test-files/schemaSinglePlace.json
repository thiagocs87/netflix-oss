{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "id",
    "name",
    "slug",
    "city",
    "state",
    "createdAt",
    "updatedAt"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        10
      ]
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "slug": {
      "$id": "#/properties/slug",
      "type": "string",
      "title": "The Slug Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "city": {
      "$id": "#/properties/city",
      "type": "string",
      "title": "The City Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "state": {
      "$id": "#/properties/state",
      "type": "string",
      "title": "The State Schema",
      "default": "",
      "examples": [
        "AC"
      ],
      "pattern": "^(.*)$"
    },
    "createdAt": {
      "$id": "#/properties/createdAt",
      "type": "string",
      "title": "The Createdat Schema",
      "default": "",
      "examples": [
        "2019-08-18T19:53:07.124"
      ],
      "pattern": "^(.*)$"
    },
    "updatedAt": {
      "$id": "#/properties/updatedAt",
      "type": "string",
      "title": "The Updatedat Schema",
      "default": "",
      "examples": [
        "2019-08-18T19:53:07.124"
      ],
      "pattern": "^(.*)$"
    }
  }
}
